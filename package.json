{
  "name": "test-drive",
  "version": "0.5.0",
  "description": "Opinionated library for writing web component tests",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "tsc",
    "prepublish": "npm run build",
    "test": "npm run test:node && npm run test:browser",
    "test:node": "mocha --compilers ts:ts-node/register,tsx:ts-node/register ./test/index.node.ts",
    "test:browser": "karma start --single-run",
    "start": "webpack-dev-server --progress"
  },
  "dependencies": {
    "@types/chai": "^4.0.1",
    "@types/chai-as-promised": "^0.0.31",
    "@types/chai-dom": "^0.0.5",
    "@types/sinon": "^2.3.2",
    "@types/sinon-chai": "^2.7.28",
    "chai": "^4.0.2",
    "chai-as-promised-compat": "^7.0.3",
    "chai-dom": "^1.5.0",
    "dom-matches": "^2.0.0",
    "sinon": "^2.3.6",
    "sinon-chai": "^2.11.0"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.41",
    "core-js": "^2.4.1",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-sauce-launcher": "^1.1.0",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.4.2",
    "mocha-loader": "^1.1.1",
    "rimraf": "^2.6.1",
    "ts-loader": "^2.2.1",
    "ts-node": "^3.1.0",
    "typescript": "^2.4.1",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:wix/test-drive.git"
  },
  "author": "Jiri Tobisek <jiri@wix.com>",
  "contributors": [
    "Avi Vahl <avi.vahl@wix.com>",
    "Amir Arad <amira@wix.com>",
    "Kieran Williams <kieranw@wix.com>",
    "Lior Zisman <liorzi@wix.com>"
  ],
  "bugs": {
    "url": "https://github.com/wix/test-drive/issues"
  },
  "homepage": "https://github.com/wix/test-drive#readme",
  "license": "MIT"
}
